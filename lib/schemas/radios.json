{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "$id": "#where-do-you-live",
    "title": "Form",
    "type": "object",
    "properties": {
        "where-do-you-live": {
            "title": "Where do you live?",
            "type": "string",
            "oneOf": [
                {
                    "title": "England",
                    "enum": ["england"]
                },
                {
                    "title": "Scotland",
                    "enum": ["scotland"]
                },
                {
                    "title": "Wales",
                    "enum": ["wales"]
                },
                {
                    "title": "Northern Ireland",
                    "enum": ["northern-ireland"]
                }
            ]
        }
    },
    "additionalProperties": false,
    "required": ["where-do-you-live"]
}